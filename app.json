{
  "expo": {
    "name": "Exchange",
    "slug": "Exchange",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/kgv.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/kgv.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/kgv.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 1,
      "web": {
        "favicon": "./assets/images/kgv.png"
      },
      "jsEngine": "jsc",
      "plugins": [
        "expo-secure-store"
      ],
      "package": "com.tsilteam.Exchange"
    },
    "extra": {
      "eas": {
        "projectId": "de3fe3a0-f038-40e2-a3c2-60dfe6a9a9c7"
      }
    }
  }
}
